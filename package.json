{
	"name": "proyecto",
	"version": "1.0.0",
	"description": "",
	"main": "dist/index.js",
	"type": "module",
	"scripts": {
		"dev": "ts-node-dev -r tsconfig-paths/register --respawn --transpile-only --ignore-watch node_modules --no-notify src/index.ts",
		"build": "tsc -p tsconfig.build.json && tsc-alias -p tsconfig.build.json",
		"start": "tsc -p tsconfig.build.json && node dist/index.js",
		"lint": "eslint . --ext .ts",
		"lint:fix": "eslint . --fix",
		"typeorm": "node -r ts-node/register -r tsconfig-paths/register ./node_modules/typeorm/cli.js -d ./src/Infrastructure/Persistence/PersistenceModel/db.ts",
		"migration:generate": "npm run typeorm migration:generate ./src/Infrastructure/Persistence/Migrations/InitialMigration",
		"migration:run": "npm run typeorm migration:run",
		"migration:revert": "npm run typeorm migration:revert",
		"seed": "ts-node -r tsconfig-paths/register src/Infrastructure/Persistence/seed.ts",
		"test": "jest",
		"test:watch": "jest --watch",
		"test:coverage": "jest --coverage",
		"test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
		"mocha": "mocha --config .mocharc.json"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"dependencies": {
		"@pact-foundation/pact": "^13.2.0",
		"axios": "^1.13.2",
		"dotenv": "^17.2.3",
		"express": "^5.2.1",
		"pg": "^8.16.3",
		"reflect-metadata": "^0.2.2",
		"tsyringe": "^4.10.0"
	},
	"devDependencies": {
		"@eslint/js": "^9.39.2",
		"@types/chai": "^5.2.3",
		"@types/express": "^5.0.6",
		"@types/jest": "^30.0.0",
		"@types/mocha": "^10.0.10",
		"@types/node": "^24.10.1",
		"@types/supertest": "^6.0.3",
		"chai": "^6.2.1",
		"eslint": "^9.39.2",
		"globals": "^16.5.0",
		"jest": "^30.2.0",
		"mocha": "^11.7.5",
		"supertest": "^7.1.4",
		"ts-jest": "^29.4.6",
		"ts-node": "^10.9.2",
		"ts-node-dev": "^2.0.0",
		"tsc-alias": "^1.8.16",
		"tsconfig-paths": "^4.2.0",
		"typeorm": "^0.3.27",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.50.1"
	}
}
